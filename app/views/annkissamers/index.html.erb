<p id="notice"><%= notice %></p>

<h1>Annkissamers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @annkissamers.each do |annkissamer| %>
      <tr>
        <td><%= annkissamer.first_name %></td>
        <td><%= annkissamer.last_name %></td>
        <td><%= annkissamer.email %></td>
        <td><%= link_to 'Show', annkissamer %></td>
        <td><%= link_to 'Edit', edit_annkissamer_path(annkissamer) %></td>
        <td><%= link_to 'Destroy', annkissamer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Annkissamer', new_annkissamer_path %>
